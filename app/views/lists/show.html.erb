

<div class="hero">
  <nav>
    <div class="nav-wrapper">
      <div class="col s12">
        <a href= <%=lists_path %> class="breadcrumb">All lists</a>
        <a href= <%=list_path(@list) %> class="breadcrumb">Second</a>
      </div>
    </div>
  </nav>


<h1><%= @list.title %></h1>
<p>Description: <%= @list.description %></p>
<p>Location: <%= @list.place %></p>
<p>Completed: <%= @list.completed %></p>
<!--  -->
<div class="primary-nav">
<% @items.each do |item| %>
		<%= item.name %>
		<%= link_to "Show Item", list_item_path(@list, item), class:"waves-effect waves-light btn" %>
		<br />
	<% end %>

	<%= link_to "Add Item to List", new_list_item_path(@list), class:"waves-effect waves-light btn" %> |
</div>
<div class='primary-footer'>
<%= link_to "Add list", new_list_path, class:"waves-effect waves-light btn" %> |
<%= link_to "Edit list", edit_list_path(@list), class:"waves-effect waves-light btn" %>
<%= link_to "Delete List", list_path(@list), method: :delete, class:"waves-effect waves-light btn" %> |
<%= link_to "Back To All Lists", lists_path, class:"waves-effect waves-light btn" %>
</div>